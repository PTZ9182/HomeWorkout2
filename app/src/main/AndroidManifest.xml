<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="org.d3if2029.homeworkout">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logo_aplikasi"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_aplikasi"
        android:supportsRtl="true"
        android:theme="@style/Theme.HomeWorkout"
        tools:remove="android:appComponentFactory"
        tools:targetApi="31">
        <activity
            android:name=".ui.profile.FoodDetailActivity"
            android:exported="false" />
        <activity
            android:name=".ui.profile.NutrisiActivity"
            android:exported="false" />
        <activity
            android:name=".ui.menulatihan.SelesaiLatihanActivity"
            android:exported="false" />

        <receiver
            android:name=".NotificationReceiver"
            android:enabled="true"
            android:exported="false" />

        <activity
            android:name=".ui.menulatihan.MenuLatihanActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.menulatihan.ExerciseActivity"
            android:exported="false"  />
        <activity
            android:name=".ui.profile.BMIActivity"
            android:exported="false" />
        <activity
            android:name=".ui.splashscreen.SplasScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
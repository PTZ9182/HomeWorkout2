<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/praloginFragment">

    <fragment
        android:id="@+id/homeFragment"
        tools:layout="@layout/fragment_home"
        android:name="org.d3if2029.homeworkout.ui.home.HomeFragment"
        android:label="HomeFragment">
        <action
            android:id="@+id/action_homeFragment_to_menuLatihanActivity"
            app:destination="@id/menuLatihanActivity" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment"
        tools:layout="@layout/fragment_profile"
        android:name="org.d3if2029.homeworkout.ui.profile.ProfileFragment"
        android:label="ProfileFragment" >
        <action
            android:id="@+id/action_profileFragment_to_BMIActivity"
            app:destination="@id/BMIActivity" />
        <action
            android:id="@+id/action_profileFragment_to_nutrisiActivity"
            app:destination="@id/nutrisiActivity" />
        <action
            android:id="@+id/action_profileFragment_to_mainActivity3"
            app:destination="@id/mainActivity3" />
    </fragment>

    <activity
        android:id="@+id/menuLatihanActivity"
        android:name="org.d3if2029.homeworkout.ui.menulatihan.MenuLatihanActivity"
        android:label="activity_menu_latihan"
        tools:layout="@layout/activity_menu_latihan" />
    <fragment
        android:id="@+id/statistikFragment"
        android:name="org.d3if2029.homeworkout.ui.tanggaldanstatistik.StatistikFragment"
        android:label="fragment_statistik"
        tools:layout="@layout/fragment_statistik" >

    </fragment>

    <activity
        android:id="@+id/BMIActivity"
        android:name="org.d3if2029.homeworkout.ui.profile.BMIActivity"
        android:label="activity_bmi"
        tools:layout="@layout/activity_bmi" />
    <activity
        android:id="@+id/nutrisiActivity"
        android:name="org.d3if2029.homeworkout.ui.profile.NutrisiActivity"
        android:label="NutrisiActivity" />
    <fragment
        android:id="@+id/homeFragmentCewek"
        tools:layout="@layout/fragment_home_cewek"
        android:name="org.d3if2029.homeworkout.uicewek.homecewek.HomeFragmentCewek"
        android:label="HomeFragmentCewek">
        <action
            android:id="@+id/action_homeFragmentCewek_to_menuLatihanCewekActivity"
            app:destination="@id/menuLatihanCewekActivity" />
    </fragment>
    <activity
        android:id="@+id/menuLatihanCewekActivity"
        android:name="org.d3if2029.homeworkout.uicewek.menulatihancewek.MenuLatihanCewekActivity"
        android:label="activity_menu_latihan_cewek"
        tools:layout="@layout/activity_menu_latihan_cewek" />
    <fragment
        android:id="@+id/praloginFragment"
        android:name="org.d3if2029.homeworkout.ui.splashscreen.PraloginFragment"
        android:label="fragment_pralogin"
        tools:layout="@layout/fragment_pralogin" >
        <action
            android:id="@+id/action_praloginFragment_to_mainActivity3"
            app:destination="@id/mainActivity3" />
        <action
            android:id="@+id/action_praloginFragment_to_mainActivity2"
            app:destination="@id/mainActivity2" />
    </fragment>
    <activity
        android:id="@+id/mainActivity"
        android:name="org.d3if2029.homeworkout.MainActivity"
        android:label="MainActivity" />
    <activity
        android:id="@+id/mainActivity2"
        android:name="org.d3if2029.homeworkout.MainActivity2"
        android:label="activity_main2"
        tools:layout="@layout/activity_main2" />
    <activity
        android:id="@+id/mainActivity3"
        android:name="org.d3if2029.homeworkout.MainActivity3"
        android:label="activity_main3"
        tools:layout="@layout/activity_main3" />
</navigation>